SET(GCC_COVERAGE_COMPILE_FLAGS
(continuacao) "-fprofile-arcs -ftest-coverage")
SET(GCC_COVERAGE_LINK_FLAGS
(continuacao) "-lgcov")
SET(CMAKE_CXX_FLAGS
(continuacao) "${CMAKE_CXX_FLAGS}
(continuacao) ${GCC_COVERAGE_COMPILE_FLAGS}" )
SET(CMAKE_EXE_LINKER_FLAGS
(continuacao) "${CMAKE_EXE_LINKER_FLAGS}
(continuacao) ${GCC_COVERAGE_LINK_FLAGS}" )
SET(CMAKE_CXX_OUTPUT_EXTENSION_REPLACE 1)
